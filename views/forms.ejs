<%- include("partials/header") %>
  <%- include("partials/navbar") %>
    <div class="container mt-4">
      <div>
        <form action="/addStudent" method="post">
          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="inputEmail4">Name</label>
              <input name="name" type="text" class="form-control" id="inputEmail4" placeholder="Name">
            </div>
            <div class="form-group col-md-6">
              <label for="inputPassword4">Age</label>
              <input name="age" type="text" class="form-control" id="inputPassword4" placeholder="Age">
            </div>
          </div>
          <div class="form-group col-md-6">
            <label for="inputAddress">Course</label>
            <input name="course" type="text" class="form-control" id="inputAddress" placeholder="Course">
          </div>
          <div class="form-group col-md-6">
            <label for="inputAddress2">Year of completion</label>
            <input name="year_of_completion" type="text" class="form-control" id="inputAddress2" placeholder="Year of completion">
          </div>
          <button type="submit" class="btn btn-primary mt-2" >Submit</button>
        </form>
        </div>
      </div>

      <%- include("partials/closingTags") %>